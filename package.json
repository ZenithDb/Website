    "react-dom": "18.2.0",
    "react-instantsearch-dom": "^6.37.0",
    "react-intersection-observer": "^9.4.0",
    "react-paginate": "^8.1.3",
    "react-share": "^4.4.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "remark-gfm": "^1.0.0",
